<% if @user %>
  <h1>Welcome, <%= @user.email %></h1>
<% end %>



<form action="/users/new" method="get">
  <input type='submit' value="Sign up" />
</form>

<form action='/peep' method='post'>
  <input type='text' name='peep'>
  <input type='submit' value='Peep'>
</form>

<section>
  <% @peeps.each do |peep| %>
    <li><%= peep.peep %>  </li>
    <li><%= peep.created_at%> </li>
  <% end %>
</section>
